#include<iostream>

using namespace std;

int n;
long long cache[10000001];
long long search()
{
	cache[1] = 1;
	cache[2] = 2;

	for (int i = 3; i <= n; i++)
		cache[i] = (cache[i - 2] + cache[i - 1])%15746;

	return cache[n];
}
int main(int argc, char*argv[])
{
	cin >> n;

	cout << search() << endl;
	return 0;
}
